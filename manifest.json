{
    "manifest_version": 3,
    "name": "标签页管理器",
    "version": "1.0",
    "description": "管理和搜索浏览器标签页",
    "permissions": ["tabs"],
    "action": {
        "default_popup": "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "commands": {
        "_execute_action": {
            "suggested_key": {
                "default": "Ctrl+Shift+T"
            },
            "description": "打开标签页管理器"
        }
    }
}